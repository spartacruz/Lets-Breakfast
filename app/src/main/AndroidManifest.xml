<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.letsbreakfast">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OrderFoodMenuActivity"
            android:label="Order Menu"/>

        <activity android:name=".OrderFood1Activity"
            android:label="Aneka Nasi"/>

        <activity android:name=".PurchaseHasBeenMadeActivity"
            android:label="Completed!"/>

        <activity android:name=".FillInDetailActivity"
            android:label="Fill In Detail"></activity>
    </application>

</manifest>